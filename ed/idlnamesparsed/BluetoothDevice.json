      },
      "fragment": "BluetoothDevice includes BluetoothDeviceEventHandlers;"
    },
    {
      "spec": {
        "title": "Web Bluetooth",
        "url": "https://webbluetoothcg.github.io/web-bluetooth/"
      },
      "fragment": "BluetoothDevice includes CharacteristicEventHandlers;"
    },
