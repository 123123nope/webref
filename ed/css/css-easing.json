      "type": "type",
      "values": [
        {
          "name": "linear",
          "prose": "The linear keyword produces a linear easing function with two points:",
          "type": "value",
          "value": "linear"
        }
      ]
    },
    {
      "name": "<linear-easing-function>",
      "type": "type"
    },
