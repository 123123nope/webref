{
  "spec": {
    "title": "Web MIDI API",
    "url": "https://webaudio.github.io/web-midi-api/"
  },
  "events": [
    {
      "type": "statechange",
      "targets": [
        "MIDIAccess"
      ],
      "src": {
        "format": "IDL eventHandler"
      }
    },
    {
      "type": "statechange",
      "targets": [
        "MIDIPort"
      ],
      "src": {
        "format": "IDL eventHandler"
      }
    },
    {
      "type": "midimessage",
      "targets": [
        "MIDIInput"
      ],
      "src": {
        "format": "IDL eventHandler"
      }
    }
  ]
}